{
  "manifest_version": 3,
  "name": "KKTIX Helper",
  "version": "1.0",
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "content_scripts": [],
  "action": {
    "default_popup": "options.html",
    "default_title": "KKTIX Helper"
  }
}
